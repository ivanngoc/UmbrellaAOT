{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime ": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ListenAddr": "0.0.0.0",
  "ListenPort": 5555,
  "TlsEnabled": true,
  "TlsCrt": "certs/server.crt",
  "TlsKey": "certs/server.key",
  "Filters": [
    {
      "field": "type",
      "operator": "equals",
      "value": "event"
    },
    {
      "field": "level",
      "operator": "not_equals",
      "value": "debug"
    }
  ]
}
